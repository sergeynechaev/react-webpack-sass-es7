{
  "name": "react-webpack-sass-es7",
  "version": "1.0.0",
  "description": "Starter template for ReactJS using webpack and ES7 (babel)",
  "main": "src/App/index.js",
  "scripts": {
    "postinstall": "bower install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run dev_server & npm run dev_jade",
    "dev_server": "webpack-dev-server --inline --hot --content-base ./build",
    "dev_build": "jade -o ./build -O `2>&1 >/dev/null webpack` ./src",
    "dev_jade": "jade -o ./build -O '{\"assetsByChunkName\":{\"main\":\"assets/bundle.js\"}}' -w ./src/*.jade",
    "start": "webpack-dev-server --hot --progress --colors",
    "build": "webpack --progress --colors & npm run dev_jade",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js"
  },
  "keywords": [
    "react",
    "es6",
    "babel",
    "webpack"
  ],
  "author": "Sergey Nechaev",
  "license": "ISC",
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "node-sass": "^3.3.3",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.0-rc3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2"
  }
}
