{
  "name": "react-webpack-starter",
  "version": "0.1.0",
  "description": "Starter template for ReactJS using webpack, sass & ES7 (babel)",
  "main": "src/scripts/index.js",
  "scripts": {
    "clean": "rimraf build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run dev_server",
    "dev_server": "webpack-dev-server --inline --hot --content-base ./build",
    "_dev_server": "webpack-dev-server",
    "start": "webpack-dev-server --hot --progress --colors",
    "build": "webpack --progress --colors",
    "prod": "NODE_ENV=production webpack -p --progress --colors",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js"
  },
  "keywords": [
    "react",
    "es7",
    "babel",
    "webpack",
    "sass"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sergeynechaev/react-webpack-starter.git"
  },
  "author": "Sergey Nechaev",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-check-es2015-constants": "^6.3.13",
    "babel-plugin-react-autoprefix": "latest",
    "babel-plugin-syntax-decorators": "latest",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "latest",
    "babel-plugin-transform-class-constructor-call": "latest",
    "babel-plugin-transform-decorators": "latest",
    "babel-plugin-transform-decorators-legacy": "latest",
    "babel-plugin-transform-es2015-arrow-functions": "^6.4.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.4.0",
    "babel-plugin-transform-es2015-destructuring": "latest",
    "babel-plugin-transform-flow-strip-types": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-plugin-transform-runtime": "latest",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "latest",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-runtime": "^5.8.24",
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "history": "^2.0.1",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.0.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
